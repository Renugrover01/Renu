<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Shine Click Event tracking" verbose="3">
	<listeners>
		<listener
			class-name="com.shine.listener.TestNGCustomReportListener" />
	</listeners>


	<test name="Desktop Click event tracking">
		<classes>
			<class name="com.shine.tests.Test_RegistrationComplete"></class>
		</classes>
	</test>

	<test name="Mobile Click event tracking">
		<classes>
			<class name="com.shine.m.tests.Test_RegistrationComplete"></class>
		</classes>
	</test>
	
	<test name="Update Excel">
		<classes>
			<class name="com.shine.tests.utility.Test_UpdateRegClickEventData"></class>
		</classes>
	</test>

</suite>